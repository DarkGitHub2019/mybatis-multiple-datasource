<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lyle.sample.mybatis.mapper.test1.EmplyeeMapper">

    <select id="queryEmployeeByDeptId" resultType="com.lyle.sample.mybatis.model.Employee">
        SELECT id,name,age,dept_id FROM test_emplyee WHERE dept_id=#{deptId}
    </select>
</mapper>
